636e\u7684\u5c0f\u5757\u5185\u5b58\u7a7a\u95f4\u5b9a\u4e49\u7684\u540d\u5b57 \u53d8\u91cf\u547d\u540d 1.\u5b57\u6bcd\u3001\u6570\u5b57\u3001\u4e0b\u5212\u7ebf\u7ec4\u6210\u3002 2.\u4e0d\u80fd\u662f\u6570\u5b57\u5f00\u5934\u3002 3.\u533a\u5206\u5927\u5c0f\u5199\u3002 4.\u4e0d\u53ef\u4ee5\u4f7f\u7528 python \u5173\u952e\u8bcd 3. dict \u5b57\u5178 \u5217\u8868 \u5217\u8868\u7684\u5143\u7d20\u5e8f\u53f7\uff0c\u81ea\u5de6\u5230\u53f3\u4f9d\u6b21\u4e3a0 1 2 3 ...\uff0c \u5143\u7d20\u7684\u5e8f\u53f7\u5c31\u662f\u5143\u7d20\u7d22\u5f15\u503c\u3002 List = [] # \u5b9a\u4e49\u53d8\u91cfList\u4e3a\u4e00\u4e2a\u5217\u8868\u53d8\u91cf\u3002 List.append(\"0\") # \u6dfb\u52a0\u5143\u7d20\"0\" \u5230\u5217\u8868\u7684\u7ed3\u5c3e\u5904\u3002 List.pop() # \u5220\u9664\u5217\u8868\u5c3e\u90e8\u76841\u4e2a\u5143\u7d20\u3002 List.remove(\"0\") #\u5220\u9664\u5217\u8868\u91cc\u9762\u7684\"0\"\u5143\u7d20\u3002 List.index(\"a\") #\u67e5\u627e\u5217\u8868List\u91cc\u9762\u7684\u5143\u7d20\"a\" \u7684\u7d22\u5f15\u503c\u3002 List.insert(2,\"abc\") # \u7d22\u5f15\u4e3a2\u7684\u4f4d\u7f6e\uff0c\u63d2\u5165\u5143\u7d20\"abc\"\u3002 del List[3] # \u5220\u9664\u5217\u8868List\u91cc\u9762\uff0c\u7d22\u5f15\u4e3a3\u7684\u5143\u7d20\u3002 List[4] = 456 # \u66f4\u65b0\u7d22\u5f15\u4e3a4\u7684\u5143\u7d20\u4e3a 456 L2 = List.copy() #L2 \u662f\u590d\u5236List \u5b57\u5178 \u5b57\u5178\u7684\u6570\u636e\u662f\u4ee5\u952e\u503c\u5bf9\u51fa\u73b0\u7684\uff0c\u591a\u4e2a\u952e\u503c\u5bf9\u4e4b\u95f4\u7528\u9017\u53f7\u9694\u5f00\u3002 Dic = {\"\u5e97\u540d\":\"\u5927\u826f\u65d7\u8230\u5e97\",\"\u7c7b\u76ee\":\"\u6237\u5916\u7528\u54c1\",\"\u70e7\u70e4\u67b6\":50,\"\u5e10\u7bf7\":99} # \u5b9a\u4e49\u53d8\u91cfDic\u4e3a\u4e00\u4e2a\u5b57\u5178\u7c7b\u578b\u7684\u53d8\u91cf\u3002 Dic[\"\u5e97\u540d\"] #\u67e5\u627e\u5173\u952e\u5b57\u4e3a\u5e97\u540d\u7684\u503c\u3002 Dic[\"\u5e97\u540d\"] = \"\u76f4\u8425\u5e97\" #\u66f4\u65b0\u5173\u952e\u5b57\u4e3a\u5e97\u540d\u7684\u503c\u4e3a\"\u76f4\u8425\u5e97\"\uff1b \u5982\u679c\u4e0d\u5b58\u5728\u5e97\u540d\u8fd9\u4e2a\u5173\u952e\u5b57\uff0c\u5219\u5728\u5b57\u5178Dic\u91cc\u9762\u65b0\u589e\u4e00\u4e2a\"\u5e97\u540d\"\u7684\u952e\uff0c\u5e76\u628a\u5b83\u7684\u503c\u8bbe\u7f6e\u4e3a\"\u76f4\u8425\u5e97\"\u3002 4. type \u7c7b\u578b\u67e5\u8be2 \u5feb\u6377\u952e Ctrl + C \u590d\u5236 Ctrl + V \u7c98\u8d34 Ctrl + S \u624b\u52a8\u4fdd\u5b58\u6587\u4ef6 Ctrl + D \u590d\u5236\u5f53\u524d\u5149\u6807\u6240\u5728\u884c\uff0c\u6216\u8005\u5df2\u9009\u4e2d\u5185\u5bb9 \u6570\u636e\u7c7b\u578b \u5e38\u89c1\u6570\u636e\u7c7b\u578b\u6709 int \u6574\u6570\u3002 \u4e0d\u5e26\u5c0f\u6570\u70b9 float\u6d6e\u70b9\u6570\u3002\u5e26\u5c0f\u6570\u70b9 str \u5b57\u7b26\u4e32\u3002 \u4e00\u4e32\u6587\u672c\u5185\u5bb9 bool \u5e03\u5c14\u503c\u3002 \u903b\u8f91\u7c7b\u578b\uff0c\u771f\u6216\u5047\uff0c\u771f True ,\u5047False dict \u5b57\u5178\u3002 \u6570\u636e\u662f\u952e\u503c\u5bf9\u5f62\u5f0f\u51fa\u73b0\u3002\u65e0\u5e8f\uff0c\u6839\u636e\u952e\u53d6\u503c\u3002 list \u5217\u8868\u3002 \u6570\u636e\u662f\u6309\u987a\u5e8f\u6392\u5217\u3002\u6709\u5e8f\uff0c\u6839\u636e\u7d22\u5f15\u503c\u53d6\u5bf9\u5e94\u7684\u503c\u3002\u5de6\u5230\u53f3\uff0c\u7b2c1\u987a\u4f4d(\u987a\u5e8f\u7684\u4f4d\u7f6e)\u7684\u7d22\u5f15\u662f0. \u8fd0\u7b97\u7b26\u53f7 \u52a0\u51cf\u548c\u6570\u5b66\u7b26\u53f7\u4e00\u81f4\u3001\u4e58\u662f* \u3001\u9664\u662f/\u3001\u6c42\u4f59\u662f% \u5177\u4f53\u7b26\u53f7\u662f + \u52a0 - \u51cf * \u4e58 / \u9664 % \u6c42\u4f59 \u6570\u636e\u7c7b\u578b\u8f6c\u6362 \u6574\u6570\u3001\u6d6e\u70b9\u6570\u3001\u5b57\u7b26\u4e32\u53ef\u4ee5\u4e92\u76f8\u8f6c\u6362\u3002\u6574\u6570\u6216\u6d6e\u70b9\u6570\u53ef\u4ee5\u8fd0\u7b97\uff0c\u5b57\u7b26\u4e32\u4e0d\u53ef\u4ee5\u8fdb\u884c\u8fd0\u7b97\u3002 \u5b57\u7b26\u4e32\u8f6c\u6574\u6570\u65b9\u6cd5 int(str) \u5b57\u7b26\u4e32\u8f6c\u6d6e\u70b9\u6570\u65b9\u6cd5 float(str) input\u8f93\u5165\u7684\u6570\u636e\u7c7b\u578b input\u51fd\u6570\u8f93\u5165\u7684\u6570\u636e\uff0c\u9ed8\